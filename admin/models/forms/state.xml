<?xml version="1.0" encoding="utf-8"?><form><!--Project : jSchuetze for Joomla Filename: /admin/models/forms/title.xmlVersion : 1.1.4--><field  name = "id"            type = "hidden"/><field  name = "ordering"        type = "hidden"/><!--     Felder:    id , name, rank , ordering , published -->    <field      name        = "name"      type        = "text"      label       = "COM_JSCHUETZE_NAME"     description = "COM_JSCHUETZE_NAME_DESC"      class       = "inputbox"      size        = "128"       required    = "true" /><field    name        = "published"     type        = "radio"    label       = "COM_JSCHUETZE_PUBLISHED"     description = "COM_JSCHUETZE_PUBLISHED_DESC"     default     = "1"     class       = "btn-group">    <option value="1">JYES</option>    <option value="0">JNO</option></field></form>