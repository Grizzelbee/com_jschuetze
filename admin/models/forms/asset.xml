<?xml version="1.0" encoding="utf-8"?><form><!--Project : jSchuetze for Joomla Filename: /admin/models/forms/asset.xmlVersion : 1.1.4--><field  name = "id"            type = "hidden"/><field  name = "ordering"        type = "hidden"/><!--     Felder:    id , name, anzahl, bestand , ordering , published -->    <field      name        = "name"      type        = "text"      label       = "COM_JSCHUETZE_ITEM"     description = "COM_JSCHUETZE_ITEM_DESC"      class       = "inputbox"      size        = "128"       required    = "true" /><field      name        = "fee"      type        = "text"      label       = "COM_JSCHUETZE_FEE"     description = "COM_JSCHUETZE_FEE_DESC"      class       = "inputbox"      size        = "10"       required    = "false" /><field      name        = "anzahl"      type        = "text"      label       = "COM_JSCHUETZE_ANZAHL"     description = "COM_JSCHUETZE_ANZAHL_DESC"      class       = "inputbox"      size        = "10"       required    = "true" /><field    name        = "bestand"     type        = "tetxt"    label       = "COM_JSCHUETZE_BESTAND"     description = "COM_JSCHUETZE_BESTAND_DESC"     size        = "10"       required    = "true"     class       = "inputbox"/><field    name        = "published"     type        = "radio"    label       = "COM_JSCHUETZE_PUBLISHED"     description = "COM_JSCHUETZE_PUBLISHED_DESC"     default     = "1"     class       = "btn-group">    <option value="1">JYES</option>    <option value="0">JNO</option></field></form>