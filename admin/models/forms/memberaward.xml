<?xml version="1.0" encoding="utf-8"?><form><!--Project : jSchuetze for Joomla Filename: /admin/models/forms/award.xmlVersion : 1.0.5--><field  name = "id"            type = "hidden"/><!--     Felder:    id , name, rank , ordering , published -->    <field      name        = "fk_mitglied"      type        = "member"      label       = "COM_JSCHUETZE_NAME"     description = "COM_JSCHUETZE_NAME_DESC"      class       = "inputbox"      size        = "1"       required    = "true" /><field    name        = "fk_auszeichnung"     type        = "award"    label       = "COM_JSCHUETZE_AWARD"     description = "COM_JSCHUETZE_AWARD_DESC"     default     = "1"     class       = "inputbox"    required    = "true" /><field    name        = "titel"     type        = "text"    label       = "COM_JSCHUETZE_TITEL"     description = "COM_JSCHUETZE_TITEL_DESC"     class       = "inputbox"    size        = "80"   /><field    name        = "periode"     type        = "text"    label       = "COM_JSCHUETZE_PERIODE"     description = "COM_JSCHUETZE_PERIODE_DESC"     class       = "inputbox"    size        = "80"       required    = "true" /><field    name        = "auszeichnungsdatum"     type        = "calendar"    label       = "COM_JSCHUETZE_AUSZEICHNUNGSDATUM"     description = "COM_JSCHUETZE_AUSZEICHNUNGSDATUM_DESC"     size        = "40"       class       = "inputbox"    required    = "true" /><field    name        = "zug"     type        = "text"    label       = "COM_JSCHUETZE_ZUG"     description = "COM_JSCHUETZE_ZUG_DESC"     class       = "inputbox"    size        = "80"   /><field    name        = "foto_url"     type        = "media"    label       = "COM_JSCHUETZE_FOTO"     description = "COM_JSCHUETZE_FOTO_DESC"     size        = "128"       class       = "inputbox"/></form>