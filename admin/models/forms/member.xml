<?xml version="1.0" encoding="utf-8"?><form><!--Project : jSchuetze for Joomla Filename: /admin/models/forms/member.xmlVersion : 1.1.3--><field  name = "id"        type = "hidden"/><field  name = "ordering"        type = "hidden"/><!--     Felder:    id , name, vorname , geburtstag , strasse , plz , ort, tel , mobile , email_priv , foto_url , religion , beitritt    austritt , ordering , published , fk_status, scet_mail_notification      -->    <field    name        = "fk_juser"     type        = "user"    label       = "COM_JSCHUETZE_JUSER"     description = "COM_JSCHUETZE_jUSER_DESC"     class       = "inputbox"    size        = "60"    default     = "0"/><field    name        = "fk_funktion"     type        = "function"    label       = "COM_JSCHUETZE_FUNCTION"     description = "COM_JSCHUETZE_FUNCTION_DESC"     class       = "inputbox"    size        = "1"    default     = "0"/><field      name        = "name"      type        = "text"      label       = "COM_JSCHUETZE_NAME"     description = "COM_JSCHUETZE_NAME_DESC"      class       = "inputbox"      size        = "128"       required    = "true" /><field    name        = "vorname"     type        = "text"    label       = "COM_JSCHUETZE_FIRSTNAME"     description = "COM_JSCHUETZE_FIRSTNAME_DESC"     class       = "inputbox"    size        = "128"    required    = "true" /><field    name        = "strasse"     type        = "text"    label       = "COM_JSCHUETZE_STREET"     description = "COM_JSCHUETZE_STREET_DESC"     class       = "inputbox"    size        = "128"/><field    name        = "plz"     type        = "text"    label       = "COM_JSCHUETZE_ZIPCODE"     description = "COM_JSCHUETZE_ZIPCODE_DESC"     class       = "inputbox"    size        = "10"/><field    name        = "ort"     type        = "text"    label       = "COM_JSCHUETZE_ORT"     description = "COM_JSCHUETZE_ORT_DESC"     class       = "inputbox"    size        = "32"/><field    name        = "geburtstag"     type        = "calendar"    label       = "COM_JSCHUETZE_BIRTHDAY"     description = "COM_JSCHUETZE_BIRTHDAY_DESC"     class       = "inputbox"    size        = "40"/><field    name        = "tel"     type        = "text"    label       = "COM_JSCHUETZE_TEL"     description = "COM_JSCHUETZE_TEL_DESC"     class       = "inputbox"    size        = "40"/><field    name        = "mobile"     type        = "text"    label       = "COM_JSCHUETZE_MOBILE"     description = "COM_JSCHUETZE_MOBILE_DESC"     class       = "inputbox"    size        = "40"/><field    name        = "email_priv"     type        = "text"    label       = "COM_JSCHUETZE_EMAIL_PRIV"     description = "COM_JSCHUETZE_EMAIL_PRIV_DESC"     class       = "inputbox"    size        = "40"/><field    name        = "scet_mail_notification"     type        = "radio"    label       = "COM_JSCHUETZE_SCET_EMAIL_NOTIFICATION"     description = "COM_JSCHUETZE_SCET_EMAIL_NOTIFICATION_DESC"     default     = "0"     class       = "inputbox">    <option value="1">JYES</option>    <option value="0">JNO</option></field><field    name        = "published"     type        = "radio"    label       = "COM_JSCHUETZE_PUBLISHED"     description = "COM_JSCHUETZE_PUBLISHED_DESC"     default     = "1"     class       = "inputbox">    <option value="1">JYES</option>    <option value="0">JNO</option></field><field    name        = "foto_url"    type        = "media"    label       = "COM_JSCHUETZE_FOTO"    description = "COM_JSCHUETZE_FOTO_DESC"      size        = "40"    class       = "inputbox"/><field    name        = "beitritt"     type        = "calendar"    label       = "COM_JSCHUETZE_BEITRITT"     description = "COM_JSCHUETZE_BEITRITT_DESC"     class       = "inputbox"    size        = "40"/><field    name        = "austritt"     type        = "calendar"    label       = "COM_JSCHUETZE_AUSTRITT"     description = "COM_JSCHUETZE_AUSTRITT_DESC"     class       = "inputbox"    size        = "40"/><field    name        = "fk_status"     type        = "status"    label       = "COM_JSCHUETZE_STATUS"     description = "COM_JSCHUETZE_STATUS_DESC"     class       = "inputbox"    size        = "1"    default     = "0"/><field    name        = "fk_lebenspartner"     type        = "member"    label       = "COM_JSCHUETZE_LEBENSPARTNER"     description = "COM_JSCHUETZE_LEBENSPARTNER_DESC"     class       = "inputbox"    size        = "1"    default     = "-1"/><field    name        = "beitritt_bruder"    type        = "calendar"    label       = "COM_JSCHUETZE_BEITRITT_BRUDERSCHAFT"    description = "COM_JSCHUETZE_BEITRITT_BRUDERSCHAFT_DESC"      size        = "40"    class       = "inputbox"/><field    name        = "austritt_bruder"    type        = "calendar"    label       = "COM_JSCHUETZE_AUSTRITT_BRUDERSCHAFT"    description = "COM_JSCHUETZE_AUSTRITT_BRUDERSCHAFT_DESC"      size        = "40"    class       = "inputbox"/><field    name        = "eintritt_sch_wesen"    type        = "calendar"    label       = "COM_JSCHUETZE_EINTRITT_SCHUETZENWESEN"    description = "COM_JSCHUETZE_EINTRITT_SCHUETZENWESEN_DESC"      size        = "40"    class       = "inputbox"/><field    name        = "religion"    type        = "text"    label       = "COM_JSCHUETZE_RELIGION"    description = "COM_JSCHUETZE_RELIGION_DESC"      size        = "40"    class       = "inputbox"/><field    name        = "mitgliedsnr_bruder"    type        = "text"    label       = "COM_JSCHUETZE_MITGLIEDSNR_BRUDERSCHAFT"    description = "COM_JSCHUETZE_MITGLIEDSNR_BRUDERSCHAFT_DESC"      size        = "40"    class       = "inputbox"/></form>