<?xml version="1.0" encoding="utf-8"?><form><!--Project : jSchuetze for Joomla Filename: /admin/models/forms/award.xmlVersion : 1.1.3--><field  name = "id"            type = "hidden"/><field  name = "ordering"        type = "hidden"/><!--     Felder:    id , name, rank , ordering , published -->    <field      name        = "name"      type        = "text"      label       = "COM_JSCHUETZE_NAME"     description = "COM_JSCHUETZE_NAME_DESC"      class       = "inputbox"      size        = "128"       required    = "true" /><field      name        = "icon"      type        = "media"      label       = "COM_JSCHUETZE_AWARD_ICON"     description = "COM_JSCHUETZE_AWARD_ICON_DESC"      class       = "inputbox"      size        = "128"       required    = "true" /><field    name        = "memberfiles"     type        = "radio"    label       = "COM_JSCHUETZE_MEMBERFILES"     description = "COM_JSCHUETZE_MEMBERFILES_DESC"     default     = "0"     class       = "inputbox">    <option value="1">JYES</option>    <option value="0">JNO</option></field><field    name        = "pfand"     type        = "radio"    label       = "COM_JSCHUETZE_PFAND"     description = "COM_JSCHUETZE_PFAND_DESC"     default     = "0"     class       = "inputbox">    <option value="1">JYES</option>    <option value="0">JNO</option></field><field    name        = "published"     type        = "radio"    label       = "COM_JSCHUETZE_PUBLISHED"     description = "COM_JSCHUETZE_PUBLISHED_DESC"     default     = "1"     class       = "inputbox">    <option value="1">JYES</option>    <option value="0">JNO</option></field></form>