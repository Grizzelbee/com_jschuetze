<?xml version="1.0" encoding="utf-8"?><form><!--Project : jSchuetze for Joomla Filename: /admin/models/forms/award.xmlVersion : 1.0.0--><field  name = "id"            type = "hidden"/><!--     Felder:    id , name, rank , ordering , published -->    <field      name        = "name"      type        = "text"      label       = "COM_JSCHUETZE_NAME"     description = "COM_JSCHUETZE_NAME_DESC"      class       = "inputbox"      size        = "128"       required    = "true" /><field    name        = "zugkoenig"     type        = "radio"    label       = "COM_JSCHUETZE_ZUGKOENIG"     description = "COM_JSCHUETZE_ZUGKOENIG_DESC"     default     = "1"     class       = "inputbox">    <option value="1">JYES</option>    <option value="0">JNO</option></field><field    name        = "corpskoenig"     type        = "radio"    label       = "COM_JSCHUETZE_CORPSKOENIG"     description = "COM_JSCHUETZE_CORPSKOENIG_DESC"     default     = "1"     class       = "inputbox">    <option value="1">JYES</option>    <option value="0">JNO</option></field><field    name        = "bruderkoenig"     type        = "radio"    label       = "COM_JSCHUETZE_BRUDERKOENIG"     description = "COM_JSCHUETZE_BRUDERKOENIG_DESC"     default     = "1"     class       = "inputbox">    <option value="1">JYES</option>    <option value="0">JNO</option></field><field    name        = "pfand"     type        = "radio"    label       = "COM_JSCHUETZE_PFAND"     description = "COM_JSCHUETZE_PFAND_DESC"     default     = "1"     class       = "inputbox">    <option value="1">JYES</option>    <option value="0">JNO</option></field><field    name        = "published"     type        = "radio"    label       = "COM_JSCHUETZE_PUBLISHED"     description = "COM_JSCHUETZE_PUBLISHED_DESC"     default     = "1"     class       = "inputbox">    <option value="1">JYES</option>    <option value="0">JNO</option></field></form>