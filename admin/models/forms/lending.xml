<?xml version="1.0" encoding="utf-8"?><form><!--Project : jSchuetze for Joomla Filename: /admin/models/forms/lending.xmlVersion : 1.0.7--><field  name = "id"            type = "hidden"/><field  name = "ordering"        type = "hidden"/><!--     Felder:    id , fk_schuetze, fk_fundus, anzahl_aus, ausgabe, anzahl_rueck, rueckgabe, ordering , published -->    <field    name        = "fk_schuetze"     type        = "member"    label       = "COM_JSCHUETZE_MEMBER"     description = "COM_JSCHUETZE_MEMBER_DESC"     class       = "inputbox"    size        = "1"    default     = "-1"/><field    name        = "fk_fundus"     type        = "asset"    label       = "COM_JSCHUETZE_ITEM"     description = "COM_JSCHUETZE_ITEM_DESC"     class       = "inputbox"    size        = "1"    default     = "0"/><field      name        = "anzahl_aus"      type        = "text"      label       = "COM_JSCHUETZE_ANZAHL_AUS"     description = "COM_JSCHUETZE_ANZAHL_AUS_DESC"      class       = "inputbox"      size        = "10"       required    = "true" /><field    name        = "ausgabe"    type        = "calendar"    label       = "COM_JSCHUETZE_DATUM_AUS"    description = "COM_JSCHUETZE_DATUM_AUS_DESC"      size        = "40"    class       = "inputbox"/><field      name        = "anzahl_rueck"      type        = "text"      label       = "COM_JSCHUETZE_ANZAHL_RUECK"     description = "COM_JSCHUETZE_ANZAHL_RUECK_DESC"      class       = "inputbox"      size        = "10"       required    = "false" /><field    name        = "rueckgabe"    type        = "calendar"    label       = "COM_JSCHUETZE_DATUM_RUECK"    description = "COM_JSCHUETZE_DATUM_RUECK_DESC"      size        = "40"    class       = "inputbox"/><field    name        = "published"     type        = "radio"    label       = "COM_JSCHUETZE_PUBLISHED"     description = "COM_JSCHUETZE_PUBLISHED_DESC"     default     = "1"     class       = "inputbox">    <option value="1">JYES</option>    <option value="0">JNO</option></field></form>